<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mybatis.mapper.AuditLogMapper">

    <resultMap id="AuditLogResultMap" type="com.example.mybatis.entity.AuditLog">
        <id column="id" property="id"/>
        <result column="method" property="method"/>
        <result column="http_url" property="httpUrl"/>
        <result column="request_data" property="requestData"/>
        <result column="response_data" property="responseData"/>
        <result column="ousername" property="ousername"/>
        <result column="created_at" property="createdAt"/>
    </resultMap>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
        INSERT INTO audit_log (method, http_url, request_data, response_data, ousername)
        VALUES (#{method}, #{httpUrl}, #{requestData}, #{responseData}, #{ousername})
    </insert>
</mapper>
